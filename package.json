{
  "name": "taimio-spa",
  "version": "1.0.0",
  "description": "time tracking software",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build-js && npm run build-css",
    "build-js": "browserify src/index.js -g [ babelify ] -g [ envify --NODE_ENV production ] -g [ uglifyify] -o public/bundle.js",
    "watch-js": "watchify src/index.js -t [ babelify ] -o public/bundle.js -v",
    "build-css": "lessc --clean-css=\"--s1 --advanced --compatibility=ie8\" --autoprefix=\"ie >= 8\" style.less public/style.css",
    "test": "jest",
    "lint": "eslint src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siiptuo/taimio-spa.git"
  },
  "author": "Tuomas Siipola <siiptuo@kapsi.fi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/siiptuo/taimio-spa/issues"
  },
  "homepage": "https://github.com/siiptuo/taimio-spa#readme",
  "dependencies": {
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "lodash": "^4.16.6",
    "normalize.css": "^5.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-jest": "^16.0.0",
    "babel-preset-es2015": "^6.3.13",
    "envify": "^3.4.0",
    "enzyme": "^2.3.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-react": "^6.5.0",
    "jest-cli": "^16.0.2",
    "less": "^2.6.1",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "react-addons-test-utils": "^15.3.2",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest"
  }
}
